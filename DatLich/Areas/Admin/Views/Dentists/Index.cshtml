@model IEnumerable<DatLich.Models.Dentist>

@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/AdminLayout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Dentist_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Dentist_Email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Dentist_Infor)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Dentist_Img)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Dentist_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Dentist_Email)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Dentist_Infor)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Dentist_Img)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Dentist_ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.Dentist_ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Dentist_ID })
        </td>
    </tr>
}

</table>
